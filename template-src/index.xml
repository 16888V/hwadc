<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<html b:css='false' b:defaultwidgetversion='2' b:layoutsVersion='3' expr:dir='data:blog.languageDirection' expr:lang='data:blog.locale' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
<head>


<b:comment>
=================================================
Title, meta, and link tags
================================================= </b:comment>

<b:include name='head.requiredMetaTags'/>
<b:include name='head.title'/>
<b:include name='head.allHeadContent'/>

<b:with value='&quot;https://example.com/path/to/image.png&quot;' var='image'>
<b:with value='&quot;@username_of_website&quot;' var='twitterUsernameWebsite'>
<b:with value='&quot;@username_of_content_creator&quot;' var='twitterUsernameCreator'>
  <b:include name='head.defaultTags'/>
  <b:include name='head.facebookTags'/>
  <b:include name='head.twitterTags'/>
</b:with>
</b:with>
</b:with>


<b:comment>
=================================================
CSS
================================================= </b:comment>

<b:comment>=== Core CSS ===</b:comment>
<b:include name='css.bootstrap'/>
<b:include name='css.bootsblogger'/>

<b:comment>=== Font Awesome ===</b:comment>
<b:include name='css.fontAwesome'/>

<b:comment>=== Layout mode CSS ===</b:comment>
<b:include name='head.layoutModeCss'/>

<b:comment>=== Template CSS ===</b:comment>
<b:skin>
<![CDATA[
<!--(bake style.css)-->
]]>
</b:skin>


<b:comment>
=================================================
Post layout and styles
================================================= </b:comment>

<!--(bake post-layout/_index.xml)-->


</head>
<body>
<b:class cond='data:view.isHomepage' name='blog-view-home'/>
<b:class cond='data:view.isPost' name='blog-view-item'/>
<b:class cond='data:view.isPage' name='blog-view-static'/>
<b:class cond='data:view.search.label' name='blog-view-search-label'/>
<b:class cond='data:view.search.query' name='blog-view-search-query'/>
<b:class cond='data:view.search and !data:view.search.label and !data:view.search.query' name='blog-view-search-default'/>
<b:class cond='data:view.isArchive' name='blog-view-archive'/>
<b:class cond='data:view.isError' name='blog-view-error'/>
<b:class cond='data:view.isPreview' name='blog-view-preview'/>
<b:class name='class-name'/>


<b:comment>
=================================================
Skip navigation
================================================= </b:comment>

<a class='sr-only sr-only-focusable' href='#main' id='skippy'>
  <div class='container'>
    <span class='skiplink-text'>Skip to main content</span>
  </div>
</a>


<b:comment>
=================================================
Navbar
================================================= </b:comment>

<!--(bake components/navbar.xml)-->


<b:comment>
=================================================
Jumbotron
================================================= </b:comment>

<!--(bake components/jumbotron.xml)-->


<b:comment>
=================================================
Main content
================================================= </b:comment>

<div class='main-content py-5' id='main-content'>
  <div class='container'>
    <div class='row'>
      <!-- Main -->
      <main class='main col-md-9 blogger-col-8' id='main'>
        <div class='main-wrap'>
          <b:section class='b-section-main' id='b-section-main' maxwidgets='1' name='Main' showaddelement='no'>
            <!--(bake widgets/blog-posts.xml)-->
          </b:section>
        </div>
      </main><!-- /.main -->

      <!-- Sidebar -->
      <aside class='sidebar col-md-3 blogger-col-4 mt-5 mt-md-0' id='sidebar'>
        <div class='sidebar-wrap'>
          <b:section class='b-section-sidebar wg wg-box wg-box-white wg-title-gray-dark wg-md-12 wg-sm-6 mnb-3' id='b-section-sidebar' name='Sidebar'/>
        </div>
      </aside><!-- /.sidebar -->
    </div><!-- /.row -->
  </div><!-- /.container -->
</div>


<b:comment>
=================================================
Widget 1
================================================= </b:comment>

<div class='widget-1 py-5' id='widget-1'>
  <div class='container'>
    <div class='widget-1-wrap'>
      <b:section class='b-section-widget-1 wg wg-panel wg-panel-orange wg-title-gray-dark wg-md-4 mnb-3' id='b-section-widget-1' name='Widget 1'></b:section>
    </div>
  </div>
</div>


<b:comment>
=================================================
Footer
================================================= </b:comment>

<footer class='footer py-5' id='footer'>
  <div class='container'>
    <div class='footer-wrap'>
      <ul class='list-inline'>
        <li class='list-inline-item'><a href='#'>Twitter</a></li>
        <li class='list-inline-item'><a href='#'>Facebook</a></li>
        <li class='list-inline-item'><a href='#'>Google+</a></li>
      </ul>
      <p class='mb-0'>&#169; <data:blog.title/>.</p>
      <p class='mb-0'>Template <span class='text-lowercase'><data:messages.by/></span> <a href='{{ blog.templateAuthorUrl }}'>{{ blog.templateAuthorName }}</a> + <a href='https://bootsblogger.github.io'>Bootsblogger</a>. <b:message name='messages.poweredByBloggerLink'><b:param expr:value='data:blog.bloggerUrl'/></b:message>.</p>
    </div>
  </div>
</footer>


<b:comment>
=================================================
JavaScript
================================================= </b:comment>

<b:include name='js.jquery'/>
<b:include name='js.popper'/>
<b:include name='js.bootstrap'/>

<b:comment>=== Template JavaScript ===</b:comment>
<script>
//<![CDATA[
<!--(bake script.js)-->
//]]>
</script>


<b:comment>
=================================================
Includes
================================================= </b:comment>

<!--(bake includes/_index.xml)-->


</body>
</html>