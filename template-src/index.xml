<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<HTML lang='en'>
  <head>
    <!--(bake /includes/head/head.xml)-->
  </head>
  <body expr:class='data:blog.url == data:blog.homepageUrl ? &quot;blog-page-home&quot; : data:blog.pageType == &quot;item&quot; ? &quot;blog-page-item&quot; : data:blog.pageType == &quot;static_page&quot; ? &quot;blog-page-static&quot; : data:blog.searchLabel ? &quot;blog-page-label&quot; : data:blog.searchQuery ? &quot;blog-page-search&quot; : data:blog.pageType == &quot;archive&quot; ? &quot;blog-page-archive&quot; : data:blog.pageType == &quot;error_page&quot; ? &quot;blog-page-error&quot; : &quot;blog-page-default&quot;'>


    <!-- SKIPPY
    ================================================== -->
    <a class='sr-only sr-only-focusable' href='#blog-main' id='skippy'>
      <div class='container'>
        <span class='skiplink-text'>Skip to main content</span>
      </div>
    </a>


    <!-- NAVBAR
    ================================================== -->
    <!--(bake /includes/navbar.xml)-->


    <!-- JUMBOTRON
    ================================================== -->
    <!--(bake /includes/jumbotron.xml)-->


    <!-- MAIN AND SIDEBAR
    ================================================== -->
    <div class='container blogger-container'>
      <div class='row blogger-row'>
        <!-- Main -->
        <main class='blog-main col-md-9 blogger-col-8' id='blog-main' tabindex='-1'>
          <b:section id='main-section' maxwidgets='1' showaddelement='no'>
            <!--(bake /includes/blog-posts/blog-posts.xml)-->
          </b:section>
        </main><!-- /.blog-main -->

        <!-- Sidebar -->
        <aside class='blog-sidebar col-md-3 blogger-col-4 mt-5 mt-md-0' id='blog-sidebar'>
          <b:section class='wg-box wg-box-faded wg-md-12 wg-sm-6' id='sidebar-section'></b:section>
        </aside><!-- /.blog-sidebar -->
      </div><!-- /.row -->
    </div><!-- /.container -->


    <!-- WIDGET 1
    ================================================== -->
    <div class='widget-1 mt-5' id='widget-1'>
      <div class='container'>
        <b:section class='wg-box wg-box-white wg-md-4 wg-sm-6' id='widget-1-section'></b:section>
      </div>
    </div>


    <!-- FOOTER
    ================================================== -->
    <footer class='blog-footer' id='blog-footer'>
      <div class='container'>
        <ul class='blog-footer-links'>
          <li><a href='#'>About</a></li>
          <li><a href='#'>Twitter</a></li>
          <li><a href='#'>Google+</a></li>
          <li><a href='#'>Facebook</a></li>
        </ul>
        <p>&#169; 2017 <data:blog.title/>.</p>
        <p>Template by <a href='{{ blog.about.templateAuthorURI }}'>{{ blog.about.templateAuthorName }}</a>. Built with <a href='https://bootsblogger.github.io'>Bootsblogger</a>. Powered by <a href='https://blogger.com'>Blogger</a>.</p>
      </div>
    </footer>


    <!-- JavaScript
    ================================================== -->
    <!--(bake /includes/js.xml)-->


  <!-- Remove Blogger's default JavaScript -->
  &lt;!--</body>--&gt;
  &lt;/body&gt;
</HTML>