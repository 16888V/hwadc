<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<html b:css='false' b:defaultwidgetversion='2' b:layoutsVersion='3' expr:dir='data:blog.languageDirection' expr:lang='data:blog.locale' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
<head>


<b:comment>
=================================================
Title, meta, and link tags
================================================= </b:comment>

<b:include name='requiredMetaTags'/>
<b:include name='customTitle'/>
<b:include name='customAllHeadContent'/>

<b:with value='&quot;https://example.com/path/to/image.png&quot;' var='image'>
<b:with value='&quot;@username_of_website&quot;' var='twitterUsernameWebsite'>
<b:with value='&quot;@username_of_content_creator&quot;' var='twitterUsernameCreator'>
  <b:include name='defaultTags'/>
  <b:include name='facebookTags'/>
  <b:include name='twitterTags'/>
</b:with>
</b:with>
</b:with>


<b:comment>
=================================================
CSS
================================================= </b:comment>

<b:comment>=== Core CSS ===</b:comment>
<b:include name='bootstrapCss'/>
<b:include name='bootsbloggerCss'/>

<b:comment>=== Font Awesome ===</b:comment>
<b:include name='fontAwesome'/>

<b:comment>=== Layout mode CSS ===</b:comment>
<b:include name='layoutModeCss'/>

<b:comment>=== Template CSS ===</b:comment>
<b:skin>
<![CDATA[
<!--(bake /style.css)-->
]]>
</b:skin>


<b:comment>
=================================================
JavaScript (head)
================================================= </b:comment>

<b:comment>=== Bootsblogger JavaScript (JSON) ===</b:comment>
<b:include name='bootsbloggerJsonJs'/>


<b:comment>
=================================================
Post layout and styles
================================================= </b:comment>

<b:defaultmarkups>
  <b:defaultmarkup type='Common'>
    <b:includable id='post-home'>
  <!--(bake /content/blog-posts/post-home.xml)-->
</b:includable>
    <b:includable id='post-item'>
  <!--(bake /content/blog-posts/post-item.xml)-->
</b:includable>
    <b:includable id='post-static'>
  <!--(bake /content/blog-posts/post-static.xml)-->
</b:includable>
    <b:includable id='post-search-label'>
  <!--(bake /content/blog-posts/post-search-label.xml)-->
</b:includable>
    <b:includable id='post-search-query'>
  <!--(bake /content/blog-posts/post-search-query.xml)-->
</b:includable>
    <b:includable id='post-search-default'>
  <!--(bake /content/blog-posts/post-search-default.xml)-->
</b:includable>
    <b:includable id='post-archive'>
  <!--(bake /content/blog-posts/post-archive.xml)-->
</b:includable>
  </b:defaultmarkup>
</b:defaultmarkups>


</head>
<body>
<b:class cond='data:view.isHomepage' name='blog-view-home'/>
<b:class cond='data:view.isPost' name='blog-view-item'/>
<b:class cond='data:view.isPage' name='blog-view-static'/>
<b:class cond='data:view.search.label' name='blog-view-search-label'/>
<b:class cond='data:view.search.query' name='blog-view-search-query'/>
<b:class cond='data:view.search and !data:view.search.label and !data:view.search.query' name='blog-view-search-default'/>
<b:class cond='data:view.isArchive' name='blog-view-archive'/>
<b:class cond='data:view.isError' name='blog-view-error'/>
<b:class cond='data:view.isPreview' name='blog-view-preview'/>
<b:class name='your-body-class'/>


<b:comment>
=================================================
Skip navigation
================================================= </b:comment>

<a class='sr-only sr-only-focusable' href='#main' id='skippy'>
  <div class='container'>
    <span class='skiplink-text'>Skip to main content</span>
  </div>
</a>


<b:comment>
=================================================
Navbar
================================================= </b:comment>

<!--(bake /content/navbar.xml)-->


<b:comment>
=================================================
Jumbotron
================================================= </b:comment>

<!--(bake /content/jumbotron.xml)-->


<b:comment>
=================================================
Main and sidebar
================================================= </b:comment>

<div class='main-and-sidebar py-5' id='main-and-sidebar'>
  <div class='container'>
    <div class='row'>
      <!-- Main -->
      <main class='main col-md-9 blogger-col-8' id='main'>
        <b:section class='' id='main-section' maxwidgets='1' name='Main' showaddelement='no'>
          <!--(bake /core/blog-posts/widget.xml)-->
        </b:section>
      </main><!-- /.main -->

      <!-- Sidebar -->
      <aside class='sidebar col-md-3 blogger-col-4 mt-5 mt-md-0' id='sidebar'>
        <b:section class='wg wg-box wg-box-white wg-title-gray-dark wg-md-12 wg-sm-6 mnb-3' id='sidebar-section' name='Sidebar'/>
      </aside><!-- /.sidebar -->
    </div><!-- /.row -->
  </div><!-- /.container -->
</div>


<b:comment>
=================================================
Widget 1
================================================= </b:comment>

<div class='widget-1 py-5' id='widget-1'>
  <div class='container'>
    <b:section class='wg wg-panel wg-panel-orange wg-title-gray-dark wg-md-4 mnb-3' id='widget-1-section' name='Widget 1'></b:section>
  </div>
</div>


<b:comment>
=================================================
Footer
================================================= </b:comment>

<footer class='footer py-5' id='footer'>
  <div class='container'>
    <ul class='list-inline'>
      <li class='list-inline-item'><a href='#'>Twitter</a></li>
      <li class='list-inline-item'><a href='#'>Facebook</a></li>
      <li class='list-inline-item'><a href='#'>Google+</a></li>
    </ul>
    <p class='mb-0'>&#169; <data:blog.title/>.</p>
    <p class='mb-0'>Template <span class='text-lowercase'><data:messages.by/></span> <a href='{{ blog.templateAuthorURI }}'>{{ blog.templateAuthorName }}</a> + <a href='https://bootsblogger.github.io'>Bootsblogger</a>. <b:message name='messages.poweredByBloggerLink'><b:param expr:value='data:blog.bloggerUrl'/></b:message>.</p>
  </div>
</footer>


<b:comment>
=================================================
JavaScript
================================================= </b:comment>

<b:include name='jquery'/>
<b:include name='tether'/>
<b:include name='bootstrapJs'/>
<b:include name='bootsbloggerJs'/>

<b:comment>=== Template JavaScript ===</b:comment>
<script>
//<![CDATA[
<!--(bake /script.js)-->
//]]>
</script>


<b:comment>
=================================================
Bootsblogger Core Template
================================================= </b:comment>

<!--(bake /core/core.xml)-->


</body>
</html>